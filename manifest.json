{
  "manifest_version": 3,
  "name": "Cookie & Extension Reporter",
  "version": "1.0",
  "permissions": ["cookies", "management", "storage", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
